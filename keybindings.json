  // scm
  {
    "key": "cmd+e",
    "command": "runCommands",
    "args": {
      "commands": ["list.focusFirst", "list.select"]
    },
    "when": "activeAuxiliary == 'workbench.view.scm' && auxiliaryBarFocus"
  },
  {
    "key": "cmd+i",
    "command": "runCommands",
    "args": {
      "commands": [
        "list.focusFirst",
        "list.focusDown",
        "list.focusDown",
        "list.focusDown"
      ]
    },
    "when": "activeAuxiliary == 'workbench.view.scm' && auxiliaryBarFocus"
  },
