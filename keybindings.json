  // text actions
  {
    "key": "alt+c",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "const $1 = "
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+a",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$1 = [$2]"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+a",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "const [$2] = $1"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+o",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "$1 = {$2}"
    },
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "shift+alt+o",
    "command": "editor.action.insertSnippet",
    "args": {
      "snippet": "const {$2} = $1"
    },
    "when": "editorTextFocus && !editorReadonly"
